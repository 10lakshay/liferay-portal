{
	"senna": {
		"input": "classes/META-INF/resources/node_modules",
		"output": "classes/META-INF/resources/bridge",
		"file-globs": "**/lib/**/*.js",
		"src-mod-name-mapper": {
			"from": "(.*)@[^/]*(.*)\\.js$",
			"to": "$1$2"
		},
		"dest-file-mapper": {
			"from": "(.*)\\$(.*)@.*/lib/(.*)",
			"to": "$2/src/$3"
		},
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/$1"
		}
	}
}