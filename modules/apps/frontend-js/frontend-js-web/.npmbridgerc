{
	"liferay": {
		"input": "classes/META-INF/resources",
		"output": "classes/META-INF/resources/bridge/frontend-js-web",
		"file-globs": "liferay/**/*.js",
		"src-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "frontend-js-web/$1"
		},
		"dest-file-mapper": {
			"from": "(.*)\\$(.*)@.*/lib/(.*)",
			"to": "$2/src/$3"
		},
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/frontend-js-web/$1"
		}
	}
}